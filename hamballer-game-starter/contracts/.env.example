# Contract Deployment Environment
# Copy this to .env and fill in your values

# Abstract Testnet RPC
ABSTRACT_TESTNET_RPC_URL=https://api.testnet.abs.xyz

# Private key for deployment (DO NOT commit this)
# Get test ETH from Abstract faucet: https://faucet.testnet.abs.xyz
PRIVATE_KEY=your_private_key_here

# Etherscan API key for contract verification (optional)
ETHERSCAN_API_KEY=your_etherscan_api_key

# Gas settings
GAS_PRICE=1000000000
GAS_LIMIT=8000000
